# Ignore everything in the "themes" directory, except the themes you specify
wp-content/themes/*
!wp-content/themes/underscore/

# Ignore sublime projects
*.sublime-project
*.sublime-workspace

# Ignore mac auto-generated file
.DS_Store

# Ignore src folder whithin themes files
wp-content/themes/**/src/

# Ignore dist folder whithin themes files
wp-content/themes/**/dist/

# ignore excel and csv files
wp-content/themes/**/*.xlsx
wp-content/themes/**/*.csv

# ignore bower files
bower_components/

# ignore npm files
node_modules/